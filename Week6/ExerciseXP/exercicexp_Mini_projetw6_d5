        "from llama_index.core import Settings\n",
        "\n",
        "Settings.llm = llm                    # TinyLlama\n",
        "Settings.embed_model = embed_model    # MiniLM-L6-v2\n",
        "Settings.chunk_size = 512             # taille des morceaux texte (CPU-friendly)\n",
        "Settings.chunk_overlap = 50           # chevauchement pour la continuité\n",
        "\n",
        "print(\"Settings globaux appliqués.\")"
