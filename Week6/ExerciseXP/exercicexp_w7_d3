      "cell_type": "markdown",
      "source": [
        "Calculer ROUGE par modèle"
      ],
      "metadata": {
        "id": "LkAbpTdtSmw6"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def compare_models_rouge(dataframe):\n",
        "    results = {}\n",
        "\n",
        "    for model_col in [\"generated_summary\", \"t5_base_summary\", \"gpt2_summary\"]:\n",
        "        preds = dataframe[model_col].tolist()\n",
        "        refs = dataframe[\"highlights\"].tolist()\n",
        "\n",
        "        # Prétraitement : segmenter en phrases\n",
        "        preds = [preprocess_rouge(p) for p in preds]\n",
        "        refs = [preprocess_rouge(r) for r in refs]\n",
        "\n",
        "        scores = rouge.compute(predictions=preds, references=refs)\n",
        "        results[model_col] = scores\n",
        "\n",
        "    return pd.DataFrame(results)\n"
      ],
      "metadata": {
        "id": "d8uXOyIISpfS"
      },
      "execution_count": 19,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "rouge_comparison_df = compare_models_rouge(train_sample)\n",
        "\n",
        "# Affichage lisible\n",
        "print(\"\\n📊 ROUGE scores comparés (T5-small vs T5-base vs GPT-2):\")\n",
        "print(rouge_comparison_df.round(4))\n"
      ],
