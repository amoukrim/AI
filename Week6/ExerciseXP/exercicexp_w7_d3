      "cell_type": "code",
      "source": [
        "rouge_scores = compute_rouge_score(\n",
        "    train_sample[\"generated_summary\"].tolist(),\n",
        "    train_sample[\"highlights\"].tolist()\n",
        ")\n",
        "\n",
        "print(\"🔎 Scores ROUGE :\")\n",
        "for metric, score in rouge_scores.items():\n",
        "    print(f\"{metric}: {score:.4f}\")\n"
      ],
      "metadata": {
