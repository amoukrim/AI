      ],
      "metadata": {
        "id": "WGxbo4EBFLmo"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Encodage complet : ajoute les tokens spéciaux [CLS] et [SEP]\n",
        "inputs = tokenizer.encode_plus(\n",
        "    sentence,\n",
        "    add_special_tokens=True,      # Ajoute [CLS] en début, [SEP] en fin\n",
        "    padding='max_length',         # Ajoute du padding si nécessaire\n",
        "    truncation=True,              # Coupe si trop long\n",
        "    max_length=16,                # Longueur maximale (modifiable)\n",
        "    return_tensors=\"pt\"           # Retourne des tenseurs PyTorch\n",
        ")\n",
        "\n",
        "print(\"Input IDs :\", inputs[\"input_ids\"])\n",
        "print(\"Attention Mask :\", inputs[\"attention_mask\"])\n"
      ],
